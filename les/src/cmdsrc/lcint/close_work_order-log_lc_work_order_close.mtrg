<trigger>
  <name>log lc work order close</name>
  <on-command>close work order</on-command>
  <description />
  <fire-sequence>9999</fire-sequence>
  <local-syntax>
<![CDATA[
get integrator system id
   where systyp = 'WMD'
|
sl_log event where evt_id='LC-WO-CLOSE' 
                            and wh_id = @wh_id
                            and client_id = @client_id
							and wkonum = @wkonum 
   
]]>
</local-syntax>
  <enable>yes</enable>
</trigger>