	<name>lcreprintpciplabel</name>
	<description>Re-Print PCIP Label</description>
	<type>ACTION</type>
	<ws>
		<method>POST</method>
		<href>/lcreprintpciplabel</href>
	</ws>
	<impl>
		<command>reprint lc pcip label</command>
		<arguments>
			<argument name="invtid" type="string" required="true"/>
			<argument name="wh_id" type="string" required="true"/>
